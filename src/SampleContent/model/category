'use strict'
const { Model, db } = require('spy-84-cms');

class Category extends Model {

    tableName = 'category'

    timestamps = true
    
    freezeTableName = true

    createdAt = false

    updatedAt = false

    attributes = {
        id: {
            type: this.dataTypes.INTEGER,
            autoIncrement: true,
            primaryKey: true
        },
        name: {
            type: this.dataTypes.INTEGER,
        },
    }

    constructor(db) {
        super(db)
    }


    
}   

const category = new Category(db);
category.up();

module.exports = category
