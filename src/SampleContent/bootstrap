const core = require('spy-84-cms')
const path = require('path')

core.dotenv.config()
const env = process.env;
core.configs.handle(env);

const configs = require('../configs');
const port = configs.app.server.port;
const provider = require('../app/providers');

provider.register
// http Loger
core.app.use(core.morgan('combined'));
core.app.use(core.express.static(path.join(__dirname, '../public')));

core.view;
core.route;


core.app.listen(port);

